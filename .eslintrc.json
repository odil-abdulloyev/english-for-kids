{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": "off",
        "comma-dangle": "off",
        "import/extensions": "off",
        "arrow-parens": "off",
        "no-param-reassign": "off",
        "no-new": "off"
    }
}