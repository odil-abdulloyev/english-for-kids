{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": "off",
        "no-plusplus": "off",
        "no-restricted-syntax": "off",
        "operator-linebreak": "off",
        "import/extensions": "off",
        "comma-dangle": "off",
        "function-paren-newline": "off",
        "arrow-parens": "off",
        "implicit-arrow-linebreak": "off"
    }
}
